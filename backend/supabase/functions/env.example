# Supabase Edge Functions Environment Variables Example
# Copy this file to .env and fill in your actual values

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================

# Supabase project URL
SUPABASE_URL=https://your-project-id.supabase.co

# Service role key (for admin operations - keep secret!)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Anonymous key (for client operations)
SUPABASE_ANON_KEY=your_anon_key_here

# =============================================================================
# BLOCKCHAIN CONFIGURATION
# =============================================================================

# Private key for prize distribution wallet (without 0x prefix)
# This wallet will be used to distribute prizes to winners
PRIZE_DISTRIBUTOR_PRIVATE_KEY=your_private_key_here

# RPC URL for blockchain interaction
# Local development with ngrok tunnel
RPC_URL=https://cc4ac6b176f0.ngrok-free.app

# Alternative RPC URLs
# Base Sepolia testnet
RPC_URL_BASE_SEPOLIA=https://sepolia.base.org

# Base mainnet
RPC_URL_BASE=https://mainnet.base.org

# =============================================================================
# TREASURY CONFIGURATION
# =============================================================================

# Treasury address for fee collection
TREASURY_ADDRESS=0x1C9E05B29134233e19fbd0FE27400F5FFFc3737e

# =============================================================================
# OPTIONAL: GAS SETTINGS
# =============================================================================

# Optional: Gas price settings (in Gwei)
GAS_PRICE_GWEI=20

# Optional: Gas limit for transactions
GAS_LIMIT=300000